<UserControl x:Class="Calendar.Elements.CheckboxElement"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Calendar.Elements"
             mc:Ignorable="d" Height="16" Width="150">
    <Grid>
        <CheckBox Name="CheckBox" Click="isClicked" Content="checkbox" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
